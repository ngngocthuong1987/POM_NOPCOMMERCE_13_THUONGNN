<?xml version="1.0" encoding="UTF-8"?>
<suite name="Nop Commerce" parallel="tests" thread-count="4">
  <test name="Run on FIREFOX browser">
    <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.nopcommerce.payment.Login_06_RegisterAndLogin_MultiBrowser"/>
        </classes>
  </test>
  
  <test name="Run on FIREFOX HEADLESS browser">
    <parameter name="browser" value="headless_firefox"/>
        <classes>
            <class name="com.nopcommerce.payment.Login_06_RegisterAndLogin_MultiBrowser"/>
        </classes>
  </test>
  
  <test name="Run on CHROME browser">
    <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.nopcommerce.payment.Login_06_RegisterAndLogin_MultiBrowser"/>
        </classes>
  </test>
  
  <test name="Run on CHROME HEADLESS browser">
    <parameter name="browser" value="headless_chrome"/>
        <classes>
            <class name="com.nopcommerce.payment.Login_06_RegisterAndLogin_MultiBrowser"/>
        </classes>
  </test>
</suite>
